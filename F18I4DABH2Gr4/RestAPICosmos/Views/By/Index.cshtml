@model IEnumerable<RestAPICosmos.Models.By>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ByNavn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Postnummer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ByNavn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Postnummer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ById }) |
            @Html.ActionLink("Details", "Details", new { id=item.ById }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ById })
        </td>
    </tr>
}

</table>
